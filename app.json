{
  "expo": {
    "name": "shelfie_app",
    "slug": "Shelfie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/books.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/books.png",
      "resizeMode": "contain",
      "backgroundColor": "#252231"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.shelfie-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/books.png",
        "backgroundColor": "#252231"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.shelfie_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "shelfieapp"
  }
}
